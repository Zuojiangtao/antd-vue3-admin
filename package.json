{
  "name": "antd-vue3-admin",
  "version": "1.0.0",
  "author": {
    "name": "Zuojiangtao",
    "email": "1799158837@qq.com",
    "url": "https://github.com/Zuojiangtao"
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "vite6",
    "ant-design-vue",
    "typescript",
    "ts",
    "admin",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Zuojiangtao/antd-vue3-admin.git"
  },
  "homepage": "https://github.com/Zuojiangtao/antd-vue3-admin",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:dev": "run-p build-only --mode development",
    "build:sit": "run-p build-only --mode sit",
    "build:uat": "run-p build-only --mode uat",
    "build": "run-p build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "report": "cross-env REPORT=true npm run build",
    "type-check": "vue-tsc --noEmit",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json,tsx,jsx,vue}": [
      "prettier --write",
      "eslint --cache --fix",
      "eslint"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "13.1.0",
    "@vueuse/shared": "13.1.0",
    "ant-design-vue": "4.2.6",
    "axios": "1.9.0",
    "dayjs": "1.11.13",
    "nprogress": "^0.2.0",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "1.1.0",
    "vue": "3.5.13",
    "vue-demi": "0.14.10",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@rushstack/eslint-patch": "^1.11.0",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "~5.2.3",
    "@vitejs/plugin-vue-jsx": "~4.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.1.7",
    "less": "^4.2.2",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.34.3",
    "svgo": "^4.0.0",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "~19.0.0",
    "unplugin-vue-components": "~28.4.1",
    "vite": "~6.2.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-external-cdn": "^1.0.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-svg-loader": "^5.1.0",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^1.8.10"
  }
}
